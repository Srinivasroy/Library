@model IEnumerable<Library.Models.Online_Books_Record>

@{
   
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
@*<a href=~/Views/IssuedBooks/AdminIndex.cshtml</a>*@
@*"~/Views/IssuedBooks/AdminIndex.cshtml">*@

<h2 style="color:red">Online Books Records</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Book_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Author_Name_)

        </th>
        
        <th>
            @Html.DisplayNameFor(model => model.User_Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Issued_ON)
        </th>
      
        
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Book_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Author_Name_)
            </td>
            
            <td>
                @Html.DisplayFor(modelItem => item.User_Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Issued_ON)
            </td>
            

            <td>

            </td>
        </tr>
    }

</table>
